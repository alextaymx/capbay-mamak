{
  "name": "ecom_web_v2",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev-inspect": "NODE_OPTIONS='--inspect' next dev",
    "build": "npm run codegen && next build",
    "start": "next start",
    "export": "next export",
    "clean": "rimraf \"{.next,dist,build,out,node_modules}\"",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "build-prod": "run-s clean build export",
    "build-types": "npm run codegen && tsc --noEmit --pretty",
    "prepare": "husky install",
    "lint": "next lint --cache --fix",
    "pre-commit": "npm run lint && git add -A .",
    "codegen-init": "graphql-codegen init",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.6.4",
    "@headlessui/react": "^1.6.2",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "framer-motion": "^6.3.3",
    "graphql": "^16.5.0",
    "lodash": "^4.17.21",
    "next": "12.1.6",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^11.0.0",
    "next-pwa": "^5.5.2",
    "next-seo": "^5.4.0",
    "nprogress": "^0.2.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hook-form": "^7.31.2",
    "react-icons": "^4.3.1",
    "react-toastify": "^9.0.1",
    "react-use": "^17.4.0",
    "swiper": "^8.1.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.5",
    "@graphql-codegen/typescript": "2.4.11",
    "@graphql-codegen/typescript-operations": "2.4.0",
    "@graphql-codegen/typescript-react-apollo": "3.2.14",
    "@next/bundle-analyzer": "^12.1.6",
    "@tailwindcss/forms": "^0.5.2",
    "@types/node": "17.0.35",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.24",
    "typescript": "4.6.4"
  }
}
